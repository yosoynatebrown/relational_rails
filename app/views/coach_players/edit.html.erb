<%= form_with url: "/coaches/#{@coach.id}/players/#{@player.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :MVP %>
  <%= form.check_box :MVP %>
  <%= form.label :career_total_points %>
  <%= form.text_field :career_total_points %>
  <%= form.submit "Update Player" %>
<% end %>
